{
  "name": "Min Tab",
  "version": "1.0",
  "manifest_version": 3,
  "description": "A custom new tab page with a minimalist black design.",
  "chrome_url_overrides": {
    "newtab": "new_tab.html"
  },
  "permissions": [
    "history",
    "topSites"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "search_engines.json"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "icons": {
    "128": "./assets/images/icon128.png"
  }
}